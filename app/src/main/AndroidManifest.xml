<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.phoenixchain">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="phonix chain"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.SearchAccountOfTransferActivity">
        </activity>
        <activity android:name=".home.TransferStartActivity">
        </activity>
        <activity android:name=".home.TransferDetailsActivity"/>
        <activity android:name=".me.MyOrdersActivity"/>
        <activity android:name=".me.OrderDetailActivity"/>
    </application>

</manifest>